{
  "name": "cypress-guidepup",
  "version": "1.0.0",
  "description": "A Node.js library to simplify using Guidepup virtual screen reader with Cypress for accessibility testing",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "cypress run",
    "test:open": "cypress open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cypress",
    "guidepup",
    "accessibility",
    "a11y",
    "screen-reader",
    "testing",
    "automation",
    "cypress-plugin",
    "virtual-screen-reader",
    "accessibility-testing"
  ],
  "author": "bassettsj",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bassettsj/cypress-guidepup.git"
  },
  "homepage": "https://github.com/bassettsj/cypress-guidepup#readme",
  "bugs": {
    "url": "https://github.com/bassettsj/cypress-guidepup/issues"
  },
  "peerDependencies": {
    "@guidepup/virtual-screen-reader": ">=0.32.0",
    "cypress": ">=10.0.0"
  },
  "devDependencies": {
    "@guidepup/virtual-screen-reader": "^0.32.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cypress": "^14.4.1",
    "eslint": "^8.0.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
